<span class="upvote-cotainer">
  <a href="#" class="muted upvote{{hideUpvote}}">&#x25B2;</a>
</span>

<a href="{{url}}" target="_blank">{{title}}</a>
<span class="muted">({{host}})</span>

<br>

<small class="muted">
  <span class="points">{{points}}</span> points |
  posted <time class="timeago" datetime="{{created_at}}">{{created_at}}</time> |
  <a href="#articles/{{id}}"><span class="comment-count">{{comment_count}}</span> comments</a>
</small>
